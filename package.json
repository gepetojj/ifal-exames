{
	"name": "ifal-exames",
	"description": "Releitura do website de exames do IFAL.",
	"version": "1.0.0",
	"author": "Gepetojj <contato.gepetojj@gmail.com>",
	"license": "MIT",
	"private": true,
	"homepage": "https://ifal.vercel.app/",
	"bugs": {
		"url": "https://github.com/gepetojj/ifal-exames/issues",
		"email": "contato.gepetojj@gmail.com"
	},
	"scripts": {
		"start": "remix-serve build",
		"build": "yarn build:css && remix build",
		"build:css": "postcss styles --base styles --dir app/styles --env production",
		"dev": "concurrently \"yarn dev:css\" \"node -r dotenv/config ./node_modules/@remix-run/dev/cli.js\"",
		"dev:css": "postcss styles --base styles --dir app/styles --w",
		"dev:db": "json-server --watch dev-db.json --port 3001",
		"lint": "eslint . --fix",
		"format": "prettier . --write",
		"test:cy": "cypress run",
		"postinstall": "remix setup node"
	},
	"sideEffects": false,
	"packageManager": "yarn@3.1.1",
	"dependencies": {
		"@remix-run/react": "^1.2.3",
		"@remix-run/serve": "^1.2.3",
		"@remix-run/vercel": "^1.2.3",
		"algoliasearch": "^4.12.2",
		"cleave.js": "^1.6.0",
		"fuse.js": "^6.5.3",
		"i18next": "^21.6.12",
		"i18next-browser-languagedetector": "^6.1.3",
		"m3o": "^1.0.1390",
		"nprogress": "^0.2.0",
		"qs": "^6.10.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-i18next": "^11.15.5",
		"react-icons": "^4.3.1",
		"react-instantsearch-dom": "^6.22.0",
		"remix": "^1.2.3",
		"remix-auth": "^3.2.1",
		"remix-auth-form": "^1.1.1",
		"remix-i18next": "^3.0.0",
		"tiny-invariant": "^1.2.0",
		"validator": "^13.7.0"
	},
	"devDependencies": {
		"@remix-run/dev": "^1.2.3",
		"@trivago/prettier-plugin-sort-imports": "^3.2.0",
		"@types/cleave.js": "^1.4.6",
		"@types/nprogress": "^0.2.0",
		"@types/qs": "^6.9.7",
		"@types/react": "^17.0.39",
		"@types/react-dom": "^17.0.12",
		"@types/react-instantsearch-dom": "^6.12.2",
		"@types/validator": "^13.7.1",
		"@typescript-eslint/eslint-plugin": "^5.13.0",
		"@typescript-eslint/parser": "^5.13.0",
		"autoprefixer": "^10.4.2",
		"concurrently": "^7.0.0",
		"cssnano": "^5.1.0",
		"dotenv": "^16.0.0",
		"eslint": "^8.10.0",
		"eslint-config-prettier": "^8.4.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.29.2",
		"eslint-plugin-react-hooks": "^4.3.0",
		"json-server": "^0.17.0",
		"postcss": "^8.4.7",
		"postcss-cli": "^9.1.0",
		"prettier": "^2.5.1",
		"prettier-plugin-tailwindcss": "^0.1.8",
		"tailwindcss": "^3.0.23",
		"tailwindcss-scrollbar": "^0.1.0",
		"typescript": "^4.6.2",
		"yarn-upgrade-all": "^0.6.1"
	},
	"engines": {
		"node": ">=14"
	}
